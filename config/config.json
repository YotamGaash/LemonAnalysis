{
  "logging": {
    "log_dir": "logs",
    "log_file_name": "app.log",
    "max_file_size": 5242880,
    "backup_count": 3,
    "log_level_console": "DEBUG",
    "log_level_file": "INFO"
  },
  "application": {
    "app_name": "Lemonade",
    "version": "1.0.0",
    "default_output_dir": "output"
  },
  "constants": {
    "timeout": 30,
    "retrieval_attempts": 3,
    "api_url": "https://example.com/api"
  },
  "meta": {
    "logging.log_dir": {
      "type": "str",
      "default": "logs"
    },
    "logging.log_file_name": {
      "type": "str",
      "default": "app.log"
    },
    "logging.max_file_size": {
      "type": "int",
      "default": 5242880,
      "min": 1
    },
    "logging.backup_count": {
      "type": "int",
      "default": 3,
      "min": 0
    },
    "logging.log_level_console": {
      "type": "str",
      "default": "DEBUG",
      "choices": ["DEBUG", "INFO", "WARNING", "ERROR", "CRITICAL"]
    },
    "logging.log_level_file": {
      "type": "str",
      "default": "INFO",
      "choices": ["DEBUG", "INFO", "WARNING", "ERROR", "CRITICAL"]
    },
    "constants.timeout": {
      "type": "int",
      "default": 30,
      "min": 1
    },
    "constants.retrieval_attempts": {
      "type": "int",
      "default": 3,
      "min": 1
    },
    "constants.api_url": {
      "type": "str",
      "default": ""
    }
  }
}