{
  "logging": {
    "log_dir": "logs",
    "log_file_name": "app.log",
    "max_file_size": 5242880,
    "backup_count": 3,
    "log_level_console": "DEBUG",
    "log_level_file": "INFO"
  },
  "application": {
    "app_name": "Lemonade",
    "version": "1.0.0",
    "default_output_dir": "output"
  },
  "storage_paths": {
    "sessions_path": "data/sessions",
    "screenshots_path": "data/screenshots",
    "cache_path": "data/caches",
    "raw_data_path": "data/raw",
    "processed_data_path": "data/processed",
    "proxies_path": "data/proxies"
  },
  "constants": {
    "timeout": 30,
    "retrieval_attempts": 3,
    "api_url": "https://example.com/api"
  },
  "meta": {
    "logging.log_dir": {
      "type": "str",
      "default": "logs"
    },
    "logging.log_file_name": {
      "type": "str",
      "default": "app.log"
    },
    "logging.max_file_size": {
      "type": "int",
      "default": 5242880,
      "min": 1
    },
    "logging.backup_count": {
      "type": "int",
      "default": 3,
      "min": 0
    },
    "logging.log_level_console": {
      "type": "str",
      "default": "DEBUG"
    },
    "logging.log_level_file": {
      "type": "str",
      "default": "INFO"
    },
    "constants.timeout": {
      "type": "int",
      "default": 30,
      "min": 1
    },
    "constants.retrieval_attempts": {
      "type": "int",
      "default": 3,
      "min": 1
    },
    "constants.api_url": {
      "type": "str",
      "default": "https://example.com/api"
    }
  }
}